leetcode189

java:

class Solution {
    public void rotate(int[] nums, int k) {
         while(--k >= 0){
            int tail = nums[nums.length - 1];
            for(int i = nums.length - 1; i > 0; i--){
                nums[i] = nums[i - 1];
            }

            nums[0] = tail;
        }
    }
}


php

$arr =[1,2,3,4,5,6,7];
$n = 3;
while(--$n >= 0){
        $tail = $arr[count($arr) - 1];
	for($i = count($arr) - 1; $i >= 0  ; $i--){
		$arr[$i] = $arr[$i - 1];
	}
	$arr[0] =  $tail;
}

print_r($arr);
